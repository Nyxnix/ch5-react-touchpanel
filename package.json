{
  "name": "crestron-touchpanel-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "contract:sync": "node scripts/sync-contract.mjs",
    "prebuild": "npm run contract:sync",
    "build": "vite build",
    "build:dev": "npm run contract:sync && vite build --mode development && npm run contract:stage:programming",
    "build:prod": "npm run contract:sync && vite build --mode production && npm run contract:stage:programming",
    "preview": "vite preview",
    "prearchive": "npm run contract:sync",
    "archive": "ch5-cli archive -p crestron-touchpanel-template -d dist -o archive -c ./contracts/output/CrestronTouchpanel/interface/mapping/CrestronTouchpanel.cse2j",
    "deploy:mobile": "ch5-cli deploy -p -H 0.0.0.0 -t mobile archive/crestron-touchpanel-template.ch5z",
    "deploy:xpanel": "ch5-cli deploy -p -H 0.0.0.0 -t web archive/crestron-touchpanel-template.ch5z",
    "deploy:panel": "ch5-cli deploy -p -H 192.168.1.166 -t touchscreen archive/crestron-touchpanel-template.ch5z --slow-mode",
    "build:deploy": "./scripts/build-deploy.sh",
    "contract:stage:programming": "node scripts/stage-contract-programming.mjs",
    "postbuild": "npm run contract:stage:programming",
    "contract:generate": "node scripts/generate-contract-from-config.mjs"
  },
  "dependencies": {
    "@crestron/ch5-crcomlib": "^2.11.2",
    "@crestron/ch5-webxpanel": "^2.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@crestron/ch5-shell-utilities-cli": "^2.11.2",
    "@crestron/ch5-utilities-cli": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "vite": "^5.4.10",
    "vite-plugin-static-copy": "^2.2.0"
  }
}
